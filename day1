var characterOne = ["Louis", "Admin", 19]
var characterTwo = ["Martin", "Moderator", 17]
var characterThree = ["Carlos", "Editor", 23]
var characterFour = ["Misa", "Graphic Designer", 19]

console.log("How old is each character?")
console.log(characterOne[2]) // -> 19
console.log(characterTwo[2]) // -> 17
console.log(characterThree[2]) // -> 23
console.log(characterFour[2]) // -> 19

